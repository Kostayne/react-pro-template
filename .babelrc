{
    "presets": ["@babel/env", "@babel/react", "@babel/typescript"],
    "plugins": [
        "@babel/proposal-class-properties", 
        "@babel/proposal-object-rest-spread",
        [
            "module-resolver",
            {
                "alias": {
                    "$c": "./src/components",
                    "$p": "./src/pages",
                    "$u": "./src/utils"
                }
            }
        ]
    ]
}